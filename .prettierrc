{
    "semi": true,
    "tabWidth": 2,
    "endOfLine": "lf",
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "always",
    "jsxBracketSameLine": false,
    "importOrder": [
        "^@nextui-org/(.*)$",
        "^@/components/(.*)$",
        "^@/hooks/(.*)$",
        "^@/stores/(.*)$",
        "^@/types/(.*)$",
        "^@/utils/(.*)$",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}